{
  "name": "breadstick",
  "version": "0.2.6",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "author": {
    "name": "Jonathan Bakebwa",
    "url": "https://github.com/codebender828"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "notifications",
    "toast",
    "snackbar",
    "breadstick"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codebender828/breadstick"
  },
  "scripts": {
    "babel": "yarn babel:cjs && yarn babel:es",
    "babel:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --root-mode upward src -d dist",
    "babel:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --root-mode upward src -d dist/es",
    "dev": "rollup -c --watch",
    "build": "rollup -c && rollup -c --environment MINIFY",
    "build:npm": "vue-cli-service build --target lib --name breadstick src/index.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test:unit": "vue-cli-service test:unit --passWithNoTests",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint"
  },
  "peerDependencies": {
    "animejs": "^3.1.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "breadstick": "^0.1.21",
    "core-js": "^3.3.2",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "rollup": "^1.27.12",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-uglify": "^6.0.4",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
